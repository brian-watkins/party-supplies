{
  "name": "@spheres/display",
  "version": "1.0.0",
  "description": "Easy browser-based displays",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "generate:htmlElements": "tsx ./codegen/generateHtmlElements.ts",
    "generate:svgElements": "tsx ./codegen/generateSVGElements.ts",
    "build": "rm -rf ./dist && tsc --build",
    "clean": "rm -rf ./dist && rm -rf ./tsconfig.tsbuildinfo",
    "test": "npm run test:render && npm run test:view",
    "test:view": "npm run build && ts-node --esm --swc ./behaviors/runner.ts",
    "test:view:new": "best --behaviors './behaviors/**/*.behavior.ts' --viteConfig './behaviors/vite.config.js'",
    "test:render": "best --behaviors './renderBehaviors/**/*.behavior.ts' --runInBrowser '**/*' --viteConfig ./renderBehaviors/vite.config.js --failFast",
    "test:render:debug": "VITE_DEBUG=true npm run test:render -- --showBrowser",
    "test:tsx": "tsx ./behaviors/runner.ts"
  },
  "author": "Brian Watkins",
  "license": "MIT",
  "dependencies": {
    "@spheres/store": "^1.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.35",
    "@types/express": "^4.17.17",
    "aria-attributes": "^2.0.1",
    "best-behavior": "^0.4.0",
    "esbehavior": "^4.2.3",
    "express": "^4.18.2",
    "great-expectations": "^1.5.1",
    "html-element-attributes": "^3.2.0",
    "html-tags": "^3.3.1",
    "playwright": "^1.40.0",
    "source-map-support": "^0.5.21",
    "svg-element-attributes": "^2.0.1",
    "svg-tag-names": "^3.0.1",
    "ts-morph": "^18.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
